# Compute paths
get_filename_component( PANGOLIN_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH )
SET( Pangolin_INCLUDE_DIRS "@USER_INC@" )
SET( Pangolin_INCLUDE_DIR  "@USER_INC@" )

# Library dependencies (contains definitions for IMPORTED targets)
if( NOT TARGET @LIBRARY_NAME@ AND NOT Pangolin_BINARY_DIR )
  include( "${PANGOLIN_CMAKE_DIR}/PangolinTargets.cmake" )
endif()

if(NOT ANDROID)
    # Import targets from CVars
    find_package( CVARS QUIET )
endif()

SET( Pangolin_LIBRARIES    @LIBRARY_NAME@ ${CVARS_LIBRARIES} )
SET( Pangolin_LIBRARY      @LIBRARY_NAME@ )
